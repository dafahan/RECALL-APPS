{
  "expo": {
    "name": "Recall - AI Study Assistant",
    "slug": "recall-ai-study-assistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#23220f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recall.studyassistant"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#23220f"
      },
      "package": "com.recall.studyassistant",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 1
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b7905d83-8a16-4dbc-9211-6345788e2005"
      }
    }
  }
}
